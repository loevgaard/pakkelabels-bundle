{
    "name": "loevgaard/pakkelabels-bundle",
    "description": "Symfony bundle for integrating Pakkelabels into your Symfony application",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "league/iso3166": "^2.0",
        "loevgaard/pakkelabels-php-sdk": "^1.0@dev",
        "symfony/framework-bundle": "^3.2",
        "symfony/workflow": "^3.2",
        "symfony/yaml": "^3.2",
        "symfony/form": "^3.2",
        "symfony/validator": "^3.2",
        "symfony/twig-bridge": "^3.2",
        "sensio/framework-extra-bundle": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "~5.5||~6.0"
    },
    "autoload": {
        "psr-4": {
            "Loevgaard\\PakkelabelsBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "./vendor/bin/php-cs-fixer fix --verbose --dry-run",
        "fix-style": "./vendor/bin/php-cs-fixer fix --verbose"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
